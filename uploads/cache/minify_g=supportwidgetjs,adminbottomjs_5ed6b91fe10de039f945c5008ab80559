function isNum(str)
{regexp=/(^\d+\.?\d*)|(^\d*\.?\d+)/;if(regexp.test(str))return true;else return false;}
function n_check(form,x,fieldname,i)
{var msg_addition="";error=0;for(var y=0;y<=x;y++){if(form.elements[y].name==fieldname)break;}
