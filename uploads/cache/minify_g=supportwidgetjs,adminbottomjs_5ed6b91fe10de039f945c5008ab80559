{e.preventDefault();this.execCommand(cmd,false);},keyup:function()
{this.$editor.keyup($.proxy(function(e)
{var key=e.keyCode||e.which;if(this.browser('mozilla')&&!this.pasteRunning)
{this.saveSelection();}
